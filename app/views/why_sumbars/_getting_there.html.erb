<% @getting_theres.each do |getting| %>
  <ul class="span3">
    <div class="thumbnail" style="line-height: 18px; margin-bottom: 30px;">
      <%= image_tag(getting.image.url(:tiny)) %>
      <h3 style="margin-top: 10px; margin-bottom: 10px;"><%= getting.title %></h3>
      <p>
        <%= getting.content %></p>
    </div>
  </ul>
<% end %>
